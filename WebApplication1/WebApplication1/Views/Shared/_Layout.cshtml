<!DOCTYPE HTML>
<!--ORIGINAL ASP.NET Application Example-->
<html ng-app="appName">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Title: @ViewBag.Title</title>
    <!--Original styles-->
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/sass")
    @Styles.Render("~/Content/font")

    <!--Added additional styles for new theme-->
    @Styles.Render("~/Content/assets/css")
    @Styles.Render("~/Content/assets/sass")
    @Styles.Render("~/Content/assets/fonts")

    <!--Scripts-->
    @RenderSection("styles", required: false)
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/Scripts/angular.js")
   

</head>
<body>

    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar">Github</span>
                    <span class="icon-bar">Linkedin</span>
                    <span class="icon-bar">Twitter</span>
                </button>
                @Html.ActionLink("WanderLust App", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("API", "Index", "Help", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("News", "Index", "News", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Blog", "Index", "Blog", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Contact", "Index", "Contact", new { area = "" }, null)</li>
                    <li><a href="#">Login In</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/app")
    @Scripts.Render("~/Scripts/angular-animate.js")

    @RenderSection("scripts", required: false)


</body>
</html>

   